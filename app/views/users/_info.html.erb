<h2>User info</h2>
        <%= image_tag user.get_profile_image(100,100) %>
            <table class="table">
              <tbody>
                <tr>
                  <th>name</th>
                  <th><%= user.name %></th>
                </tr>
                <tr>
                  <th>introduction</th>
                  <th><%= user.introduction %></th>
                </tr>
              </tbody>
            </table>


      <div class="social">
          <%= link_to edit_user_path(user), class: "btn btn-secondary" do %>
            <i class="fas fa-user-cog" aria-hidden="true"></i>
          <% end %>
      </div>